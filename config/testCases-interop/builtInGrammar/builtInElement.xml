<testCaseGroup xmlns="http://www.sun.com/japex/testSuite" xmlns:xi="http://www.w3.org/2001/XInclude"
    name="BuiltIn Element Grammar tests">

    <param name="org.w3c.exi.ttf.useSchemas" value="false" />
    <param name="testDocFolder" value="${japex.exi.ttfms.testCasesDir}/interop/builtInGrammar/element"/>
    <param name="reference" value="http://www.w3.org/TR/exi/#builtinElemGrammars"/>

    <testCase name="builtin.element-01">
        <param name="testDescription"  value="Root tag"/>
        <param name="japex.inputFile" value="${testDocFolder}/element-01.xml" />
    </testCase>
    <testCase name="builtin.element-02">
        <param name="testDescription"  value="Root tag with a child element"/>
        <param name="japex.inputFile" value="${testDocFolder}/element-02.xml" />
    </testCase>
    <testCase name="builtin.element-03">
        <param name="testDescription"  value="Root tag with a child element"/>
        <param name="japex.inputFile" value="${testDocFolder}/element-03.xml" />
    </testCase>
    <testCase name="builtin.element-04">
        <param name="testDescription"  value="Root tag with a child element"/>
        <param name="japex.inputFile" value="${testDocFolder}/element-04.xml" />
    </testCase>
    <testCase name="builtin.element-05">
        <param name="testDescription"  value="Promotion of a specific SE(qname) within StartTagContent grammar"/>
        <param name="japex.inputFile" value="${testDocFolder}/element-05.xml" />
    </testCase>
    <testCase name="builtin.element-06">
        <param name="testDescription"  value="Promotion of a specific SE(qname) within ElementContent grammar"/>
