<!-- =================================================================== -->
<!-- Use of xsi:type is permitted in default schema mode, even though it -->
<!-- would not have been permitted in strict schema mode. Switching from -->
<!-- complex type to another complex type.                               -->
<!-- =================================================================== -->
<foo:C xmlns:foo='urn:foo' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' 
  xsi:type='foo:B'>
  <foo:AB>a</foo:AB>
  <foo:AD>b</foo:AD>
</foo:C>
